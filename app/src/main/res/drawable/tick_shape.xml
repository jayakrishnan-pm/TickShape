<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">



    <item
        android:bottom="10dp"
        android:left="-40dp"
        android:right="0dp"
        android:top="0dp">

        <rotate
            android:pivotY="100%"
            android:pivotX="0%"
            android:fromDegrees="45"
            android:toDegrees="45">
            <shape
                android:shape="line">
                <stroke android:width="10dp" />
                <size android:height="40dp" />
                <solid android:color="#1f9789" />
            </shape>

        </rotate>
    </item>



    <item
        android:bottom="17dp"
        android:left="0dp"
        android:right="-109dp"
        android:top="0dp">

        <rotate
            android:fromDegrees="135"

            android:toDegrees="45">
            <shape xmlns:android="http://schemas.android.com/apk/res/android"
                android:shape="line">
                <stroke android:width="10dp" />
                <size android:height="170dp" />
                <solid android:color="#1f9789" />
            </shape>

        </rotate>
    </item>


</layer-list>